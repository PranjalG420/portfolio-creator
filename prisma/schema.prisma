// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./blog.db"
}

model User {
  username  String   @id
  name      String
  theme     String?
  password  String
  updatedAt DateTime @default(now()) @updatedAt
}
